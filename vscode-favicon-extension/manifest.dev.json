{
  "manifest_version": 3,
  "name": "VS Code Server Project Favicons (DEV)",
  "version": "1.0.0",
  "description": "Automatically sets unique favicons for each VS Code Server project - Development version with HTTP support",
  "permissions": ["tabs", "scripting"],
  "host_permissions": [
    "https://vs.noreika.lt/*",
    "https://favicon-api.noreika.lt/*",
    "http://192.168.110.199:8080/*",
    "http://192.168.110.199:8091/*",
    "http://localhost:8080/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://vs.noreika.lt/*",
        "http://192.168.110.199:8080/*",
        "http://localhost:8080/*"
      ],
      "js": ["content-project-favicon.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
