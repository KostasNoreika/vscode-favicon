{
  "manifest_version": 3,
  "name": "VS Code Server Project Favicons",
  "version": "6.0.3",
  "description": "Project favicons with terminal indicator, Claude notifications, and clipboard file paste (images, docs, spreadsheets)",
  "permissions": ["tabs", "scripting", "clipboardRead", "alarms", "storage", "activeTab"],
  "host_permissions": [
    "https://favicon-api.noreika.lt/*",
    "https://vs.noreika.lt/*"
  ],
  "optional_host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "VS Code Server Favicons"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html"
}
